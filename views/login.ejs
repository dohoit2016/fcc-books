<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<% include header %>
	</head>
	</head>
	<body>
		<div class="col-xs-12 col-sm-12 col-md-8 col-lg-8 col-lg-offset-2 col-md-offset-2">
		<% include nav %>
		</div>
		<div class="col-xs-12 col-sm-12 col-md-8 col-lg-8 col-lg-offset-2 col-md-offset-2">
		<div class="container">
			<div class="row">
				<div class="col-sm-12">
					<h1>Login</h1>
				</div>
				<div class="col-sm-12">
					<form class="form" name="form" method="post">
						<div class="form-group">
							<label>Email</label><input type="email" name="email" class="form-control" required="">
						</div>
						<div class="form-group">
							<label>Password</label><input type="password" name="password" class="form-control" required="">
						</div>
						<div>
							<button class="btn btn-inverse btn-lg btn-login" type="submit">Login</button>
							<a class="btn btn-default btn-lg btn-register" href="/auth/signup">Register</a>
						</div>
						<hr>
						<div>
						</div>
					</form>
				</div>
			</div>
			<hr>
		</div>
		</div>
	</body>
</html>
